@model IEnumerable<MateriaPrima>
@{
    ViewData["Title"] = "Materia Prima";
}

<h1>ESTO SOLO ES PRUEBA PARA SABER SI FUNCIONA MI LOGICA</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Materia Prima</th>
            <th scope="col">Descripción</th>
            <th scope="col">Marca</th>
            <th scope="col">Tipo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var o in Model)
        {
            <tr>
                
                <td>@o.Id</td>
                <td>@o.Nombre</td>
                <td>@o.Descripcion</td>
                <td>@o.Marca</td>
                <td>@o.Tipo</td>
            </tr>
        }
    </tbody>
</table>

